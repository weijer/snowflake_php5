{
    "name": "yiranzai/snowflake",
    "type": "library",
    "description": "This package to generate 64 bit identifier, use snowflakes from Twitter.",
    "keywords": [
        "yiranzai",
        "snowflake"
    ],
    "homepage": "https://github.com/yiranzai/snowflake",
    "license": "MIT",
    "authors": [
        {
            "name": "yiranzai",
            "email": "wuqingdzx@gmail.com",
            "homepage": "https://blog.yiranzai.cn",
            "role": "Developer"
        }
    ],
    "require": {
        "php" : "~7.1"
    },
    "require-dev": {
        "phpunit/phpunit" : ">=7.0",
        "squizlabs/php_codesniffer": "^3.0"
    },
    "autoload": {
        "psr-4": {
            "Yiranzai\\SnowFlake\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Yiranzai\\SnowFlake\\Tests\\": "tests"
        }
    },
    "scripts": {
        "test": "phpunit",
        "check-style": "phpcs src tests",
        "fix-style": "phpcbf src tests"
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.0-dev"
        }
    },
    "config": {
        "sort-packages": true
    }
}
